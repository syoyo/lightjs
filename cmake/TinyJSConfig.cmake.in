@PACKAGE_INIT@

# TinyJS CMake Configuration File
# This file provides the TinyJS::tinyjs target for use in other CMake projects

include(CMakeFindDependencyMacro)

# Find dependencies if any
# (Currently TinyJS has no external dependencies)

# Include the targets file
include("${CMAKE_CURRENT_LIST_DIR}/TinyJSTargets.cmake")

# Set variables for compatibility
set(TinyJS_FOUND TRUE)
set(TinyJS_VERSION @PROJECT_VERSION@)
set(TinyJS_INCLUDE_DIRS "@PACKAGE_CMAKE_INSTALL_INCLUDEDIR@/tinyjs")
set(TinyJS_LIBRARIES TinyJS::tinyjs)

check_required_components(TinyJS)

# Print found message
if(NOT TinyJS_FIND_QUIETLY)
  message(STATUS "Found TinyJS: ${TinyJS_VERSION}")
endif()
